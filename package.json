{
  "name": "crypto-exchanger",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=dev nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prepare:db:dev": "NODE_ENV=dev docker-compose -f ./build/docker/dc-dev.yml up",
    "prepare:db:test": "NODE_ENV=test docker-compose -f ./build/docker/dc-test.yml up",
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "migrate:dev": "NODE_ENV=dev npm run migrate",
    "unmigrate:dev": "NODE_ENV=dev npm run unmigrate",
    "start:webpack:dev": "NODE_ENV=dev webpack-dev-server --mode development",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:acceptance": "jest --end=test --testTimeout=15000 --forceExit --detectOpenHandles --testPathPattern=./test/acceptance"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@fastify/static": "^6.10.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.4.2",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^6.3.0",
    "axios": "^1.4.0",
    "config": "^3.3.9",
    "cron": "^2.3.1",
    "joi": "^17.9.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/config": "^3.3.3",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "28.1.2",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.3.2",
    "sequelize-typescript": "^2.1.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
